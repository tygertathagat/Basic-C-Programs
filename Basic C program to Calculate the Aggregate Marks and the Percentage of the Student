#include <stdio.h>

int main() 	{

int s1,s2,s3,s4,s5,agg,percentage;
printf("\nEnter the Marks of Subject 1 :");
scanf("%d",&s1);
printf("\nEnter the Marks of Subject 2 :");
scanf("%d",&s2);
printf("\nEnter the Marks of Subject 3 :");
scanf("%d",&s3);
printf("\nEnter the Marks of Subject 4 :");
scanf("%d",&s4);
printf("\nEnter the Marks of Subject 5 :");
scanf("%d",&s5);
agg=s1+s2+s3+s4+s5;
printf("\nThe Aggregate Marks of the Student are :  %d",agg);
percentage= 100* agg/500;
printf("\nThe Percentage of the Student is  : %d",percentage);
return 0;
		}
